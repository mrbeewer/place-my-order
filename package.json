{
  "name": "place-my-order",
  "version": "0.0.0",
  "description": "an order app",
  "homepage": "mrbeewer.herokuapp.com",
  "author": {
    "name": "David Beeler",
    "email": "david.beeler@gmail.com",
    "url": ""
  },
  "scripts": {
    "api": "place-my-order-api --port 7070",
    "test": "testee src/test.html --browsers firefox --reporter Spec",
    "start": "can-serve --proxy http://localhost:7070 --port 8080",
    "develop": "can-serve --develop --proxy http://localhost:7070 --port 8080",
    "document": "documentjs",
    "build": "node build"
  },
  "main": "place-my-order/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [],
  "system": {
    "envs": {
      "server-production": {
        "baseURL": "https://pmo-mrbeewer.firebaseapp.com/"
      }
    },
    "main": "place-my-order/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload"
    ],
    "npmIgnore": [
      "documentjs",
      "testee",
      "donejs-deploy",
      "generator-donejs",
      "donejs-cli"
    ]
  },
  "dependencies": {
    "bit-tabs": "^0.2.0",
    "can": "^2.3.0",
    "can-connect": "^0.3.0",
    "can-ssr": "^0.11.0",
    "done-autorender": "^0.6.0",
    "done-component": "^0.4.0",
    "done-css": "~1.1.13",
    "generator-donejs": "^0.4.3",
    "jquery": "2.1.4",
    "place-my-order-api": "^0.4.4",
    "place-my-order-assets": "^0.1.6",
    "steal": "^0.13.0",
    "steal-socket.io": "^1.0.1"
  },
  "devDependencies": {
    "documentjs": "^0.4.1",
    "donejs-deploy": "^0.4.0",
    "funcunit": "~3.0.0",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^0.13.0",
    "testee": "^0.2.4",
    "donejs-cli": "^0.6.0",
    "can-fixture": "^0.1.0"
  },
  "donejs": {
    "deploy": {
      "root": "dist",
      "services": {
        "production": {
          "type": "firebase",
          "config": {
            "firebase": "pmo-mrbeewer",
            "public": "./dist",
            "headers": [
              {
                "source": "/**",
                "headers": [
                  {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                  }
                ]
              }
            ]
          }
        }
      }
    }
  }
}
